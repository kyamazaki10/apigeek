extends ../layout

block content
    .container

        .page-header
            h1= title

        .content
            form(name='Developers' class='form-inline')
                .form-group
                    label(for='email') Email:
                    input(type='text' class='form-control')
                button(id='listDeveloperApps' class='btn btn-default submit') Submit

            h4(class='results') Result
            pre
                code(class='result-json')